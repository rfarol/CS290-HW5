<h1>You sent the following data in a POST request:</h1>
<table>
  <caption>Post Values Below:</caption>
  <thead>
    <tr>
       <th>Label</th>
       <th>Key</th>
    </tr>
  </thead>
  <tbody>
  {{#each postquerylist}}
    <tr>
        <td>{{this.label}}</td>
        <td>{{this.key}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>

<table>
  <caption>Post Body Values Below:</caption>
  <thead>
    <tr>
       <th>Label</th>
       <th>Key</th>
    </tr>
  </thead>
  <tbody>
  {{#each postbodylist}}
    <tr>
        <td>{{this.blabel}}</td>
        <td>{{this.bkey}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>